{
  "name": "meirin",
  "version": "0.3.0",
  "description": "A powerful frequency limitation library",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "play": "ts-node playground.ts",
    "build": "tsc",
    "bench": "matcha",
    "prepublish": "npm run build"
  },
  "author": "XGHeaven",
  "license": "MIT",
  "dependencies": {
    "ms": "^2.1.1",
    "ramda": "^0.25.0"
  },
  "files": ["lib", "README.md"],
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/ms": "^0.7.30",
    "@types/node": "^9.4.0",
    "@types/ramda": "^0.25.17",
    "@xgheaven/tslint-config-xgheaven": "^0.1.3",
    "jest": "^22.1.4",
    "matcha": "^0.7.0",
    "ts-jest": "^22.0.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
